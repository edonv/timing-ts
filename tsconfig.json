{
  "compilerOptions": {
    "lib": ["ES5"],
    "target": "ES5",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "lib",
    "noImplicitAny": false,
    "strict": false,
    "sourceMap": true,
    "declaration": true,
    "noUncheckedIndexedAccess": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["src/openapi/generated/**"]
}
