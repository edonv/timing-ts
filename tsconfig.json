{
  "compilerOptions": {
    "lib": ["ES2023"],
    "target": "ES2023",
    "moduleResolution": "Node16",
    "module": "Node16",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "noImplicitAny": false,
    "strict": false,
    "sourceMap": true,
    "declaration": true,
    "noUncheckedIndexedAccess": true
  },
  "include": ["src", "test"],
  "exclude": ["src/openapi/generated/**"]
}
