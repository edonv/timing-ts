{
  "name": "timing-ts",
  "version": "0.1.2",
  "type": "module",
  "license": "MIT",
  "description": "API package for Timing.",
  "repository": "github:edonv/timing-ts",
  "author": "edonv",
  "private": false,
  "sponsor": {
    "url": "https://github.com/sponsors/edonv"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "test": "tsc && node dist/test/timing.test.js",
    "api-gen": "openapi-typescript ./src/openapi/timing-openapi.yaml -o ./src/openapi/schema.ts",
    "gen-build": "npm run api-gen && npm run build",
    "publish": "npm run gen-build && npm publish"
  },
  "main": "./dist/timing.js",
  "types": "./dist/timing.d.ts",
  "files": [
    "/README.md",
    "/LICENSE.md",
    "/dist/"
  ],
  "dependencies": {
    "openapi-fetch": "^0.10.6"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.0.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "luxon": "^3.5.0",
    "openapi-typescript": "^7.3.0",
    "ts-jest": "^29.2.3",
    "typescript": "^5.5.3"
  },
  "keywords": [
    "timing",
    "openapi",
    "mac",
    "time",
    "tracking",
    "api",
    "typescript"
